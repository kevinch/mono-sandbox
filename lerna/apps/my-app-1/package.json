{
  "name": "my-app-1",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "intl-equalizer": {
    "referenceLocale": "en",
    "localeDirectory": "messages"
  },
  "scripts": {
    "prepare": "cd ./react && yarn",
    "lint": "cd ./react && tsc --noEmit && eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "locales:lint": "intl-equalizer",
    "locales:fix": "intl-equalizer --fix",
    "test": "cd ./react && yarn test"
  }
}
